{
    "name": "beedelivery/bee-acqio",
    "description": "Package for integration with Acqio",
    "keywords": ["acqio", "laravel", "bee"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bee Delivery",
            "email": "suporte@beedelivery.com.br"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "BeeDelivery\\Acqio\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BeeDelivery\\Acqio\\AcqioServiceProvider"
            ],
            "aliases": {
                "Acqio": "BeeDelivery\\Acqio\\Facades\\Acqio"
            }
        }
    }
}
